@model AspNet.Capitulo03.Portfolio.Models.PortfolioViewModel

@{
    ViewBag.Title = "Portfolio";
}

<h2>@ViewBag.Title</h2>

<div class="row">
    @foreach (var caminho in Model.CaminhoImagens)
    {
        <a href="@caminho">
            <img src="@caminho" alt="@caminho" />
        </a>
    }
</div>

@section scripts{

    <script src="~/Scripts/jquery.fancybox.js"></script>
}
